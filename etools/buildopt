# Project relevant defines
PROJNAME := stm32f7-disco

PROJEXEC := 
PROJMAP := 

PROJDEF := -DDEBUG -DTRACE -DSTM32F746xx

CFLAGS := -mcpu=cortex-m7 -mthumb -Og -Wextra -Wall -g3 \
 		   -fmessage-length=0 -fsigned-char \
 		   -ffunction-sections -fdata-sections \
 		   -ffreestanding -fno-move-loop-invariants \
		   -std=gnu11 \
		   $(PROJDEF) 
CXXFLAGS := -mcpu=cortex-m7 -mthumb -Og -Wextra -g3 \
		     -fmessage-length=0 -fsigned-char \
		     -ffunction-sections -fdata-sections \
		     -ffreestanding -fno-move-loop-invariants \
			 -std=gnu++11 -fabi-version=0 \
			 $(PROJDEF) 

ASFLAGS := -mcpu=cortex-m7 -mthumb -Og -Wextra -g3 -x assembler-with-cpp \
		   -fmessage-length=0 -fsigned-char \
		   -ffunction-sections -fdata-sections \
		   -ffreestanding -fno-move-loop-invariants  \
		   $(PROJDEF)
 
LDFLAGS = -mcpu=cortex-m7 -mthumb -Og -Wextra -g3 -nostartfiles -Xlinker --gc-sections --specs=nano.specs \
		    -fmessage-length=0 -fsigned-char \
		    -ffunction-sections -fdata-sections \
		    -ffreestanding -fno-move-loop-invariants \
		    $(LDLIST) \
		    -Wl,-Map,$(shell echo -n "$(abspath $(OUTDIR)/$(PROJNAME).map)" | sed -e "s/\/cygdrive\/\(.\)/\1:/g" -) \
		    -o $(shell echo -n "$(abspath $(OUTDIR)/$(PROJNAME).elf)" | sed -e "s/\/cygdrive\/\(.\)/\1:/g" -) \
		    $(shell echo -n "$(abspath $(OBJDIR)/*.o)" | sed -e "s/\/cygdrive\/\(.\)/\1:/g" -) \
		    $(shell echo -n "$(abspath $(USEROBJS))" | sed -e "s/\/cygdrive\/\(.\)/\1:/g" -) \
		    $(shell echo -n "$(abspath $(USERLIBS))" | sed -e "s/\/cygdrive\/\(.\)/\1:/g" -)

